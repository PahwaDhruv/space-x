{
  "name": "space-x",
  "version": "1.0.0",
  "description": "SpaceX launch programs",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"babel-node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "prod:build-server": "webpack --config webpack.server.js",
    "prod:build-client": "webpack --config webpack.client.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm-run-all --parallel prod:*"
  },
  "author": "Dhruv Pahwa",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "compression": "^1.7.4",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1",
    "style-loader": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
